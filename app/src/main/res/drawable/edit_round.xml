<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    >

    <!--Şeklin iç rengini belirtir. #FFFF beyaz renk kullanılır.-->
    <solid android:color="#ffff"/>

    <!--Şeklin kenarlığının rengini ve kalınlığını belirtir.
    #090909 siyah renk ve 1dp kalınlık kullanılır. -->
    <stroke
        android:color="#090909"
        android:width="1dp"/>
</shape>